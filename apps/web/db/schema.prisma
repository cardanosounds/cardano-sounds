generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Metadata {
  token_name          String @id @unique
  player              String
  image               String
  probability         String
  rarity              String
  // track               Track  @relation(fields: [trackEnriching, trackMelody, trackDrums, trackBass, trackSignature], references: [enriching, melody, drums, bass, signature])
  arweave_id_sound    String
  ipfs_id_sound       String
  arweave_website_uri String
  trackEnriching      String
  trackMelody         String
  trackDrums          String
  trackBass           String
  trackSignature      String
}

