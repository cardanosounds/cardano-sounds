generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

model Metadata {
  id                  Int    @id @default(autoincrement())
  token_name          String @unique
  player              String
  image               String
  probability         String
  rarity              String
  arweave_id_sound    String
  ipfs_id_sound       String
  arweave_website_uri String
  trackEnriching      String
  trackMelody         String
  trackDrums          String
  trackBass           String
  trackSignature      String
  collection          String
}
